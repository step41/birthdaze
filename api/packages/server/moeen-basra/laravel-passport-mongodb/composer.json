{
    "name": "moeen-basra/laravel-passport-mongodb",
    "description": "Laravel Passport provides OAuth2 server support to Laravel.",
    "keywords": ["laravel", "passport", "oauth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Moeen Basra",
            "email": "m.basra@live.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "firebase/php-jwt": "^6.2",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/auth": "^9.17",
        "illuminate/console": "^9.17",
        "illuminate/container": "^9.17",
        "illuminate/contracts": "^9.17",
        "illuminate/database": "^9.17",
        "illuminate/encryption": "^9.17",
        "illuminate/http": "^9.17",
        "illuminate/support": "^9.17",
        "league/oauth2-server": "^8.3",
        "phpseclib/phpseclib": "^3.0",
        "phpseclib/phpseclib2_compat": "^1.0",
        "symfony/psr-http-message-bridge": "^2.1",
        "laminas/laminas-diactoros": "^2.1"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "MoeenBasra\\LaravelPassportMongoDB\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "MoeenBasra\\LaravelPassportMongoDB\\PassportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
